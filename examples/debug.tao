class VecData A =
    => Proj

data Three

for A member Three of VecData A =
    => Proj = { x : A, y : A, z : A }

data Vec A, N < VecData A = N.Proj

def v3 : Vec Nat Three = Vec {
    x : 4,
    y : 5,
    z : "hi",
}

