import "lib/std.tao"

data Zero

data Succ A

class Number =
    => nat : Nat

member Zero of Number =
    => nat = 0

for A < Number member Succ A of Number =
    => nat = A.nat + 1

class TAdd B =
    => Result

for B member Zero of TAdd B =
    => Result = B

for A < TAdd B, B member Succ A of TAdd B =
    => Result = Succ A.Result

def main = <Succ (Succ Zero)>.nat
