def has_factors = n -> x ->
	if x * x > n
	then false
	else if n % x = 0
		then true
		else has_factors(n, x + 1)

def is_prime = x -> !has_factors(x, 2)

def primes_in_range of Num -> Num -> Num = a -> b ->
	if a = b
	then 0
	else
		primes_in_range(a + 1, b) + if a:is_prime
		then 1
		else 0

def main = primes_in_range(2, 10000)
