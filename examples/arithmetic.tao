given A type Succ = (A, ())

type Zero = Succ ()
type One = Succ Zero
type Two = Succ One
type Three = Succ Two
type Four = Succ Three

def foo = |x of Four, y of Succ (Succ (Succ (Succ Zero)))|
	# Hacky equivalence check
	[x] ++ [y]

type Universe = ()

given A type IO = Universe -> (A, Universe)

def main = |x, y| if x = y then true else false
