def len A of [A] -> ? = |xs| match xs in
| [_, tail: ...] => 1 + tail:len
| _ => 0

def main = match (.a: 5, .b: 3) in
| (.a: 3, .b: 5) => (a, b)
| (.a, .b) => (a, b)
