type Person = {
	name: Str,
	age: Num,
}

def make_person of ? -> ? -> Person = |name, age| { name, age }

def bob = make_person("Bob", 35)

def steve =
	let { name, age } = bob in
	{ name: "Steve", age }

def len A of [A] -> ? = |xs| match xs in
	[_, tail: ...] => 1 + tail:len,
	_ => 0,

def main = steve.name
